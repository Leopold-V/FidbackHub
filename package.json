{
    "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend/app && npm run develop",
    "dev:widget": "cd widget && npm run dev",
    "build:widget": "cd widget && npm run build",
    "build:frontend": "cd frontend && yarn build",
    "start": "npm run dev:backend & npm run dev:frontend",
    "release": "release-it",
    "format:check": "prettier -c \"frontend/**/*.{js,ts,jsx,tsx}\" \"widget/**/*.{js,ts,jsx,tsx}\"",
    "format:fix": "prettier -w \"frontend/**/*.{js,ts,jsx,tsx}\" \"widget/**/*.{js,ts,jsx,tsx}\""
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@next-auth/typeorm-legacy-adapter": "^2.0.1"
  }
}
